   "source": [
    "Enumerate takes a **start** value as starting index (if not 0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "week_days = [\"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\", \"Saturday\", \"Sunday\"]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1: Monday\n",
      "2: Tuesday\n",
      "3: Wednesday\n",
      "4: Thursday\n",
      "5: Friday\n",
      "6: Saturday\n",
      "7: Sunday\n"
     ]
    }
   ],
   "source": [
    "for index, day in enumerate(week_days, start=1):\n",
    "    print(f\"{index}: {day}\")"
   ]
